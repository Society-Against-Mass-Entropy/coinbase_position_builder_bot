{
  "env": {
    "node": true,
    "es6": true
  },
  "extends": "eslint:recommended",
  "globals": {
  },
  "parserOptions": {
    "ecmaFeatures": {
      "blockBindings": true,
      "experimentalObjectRestSpread": true
    },
    "ecmaVersion": 2018
  },
  "ignorePatterns": ["*.spec.js", "node_modules/"],
  "rules": {
    "camelcase": 0,
    "comma-spacing": 0,
    "quotes": [
      0,
      "single"
    ],
    "new-cap": 0,
    "no-console": 0,
    "no-empty": 0,
    "no-extend-native": 0,
    "no-multi-spaces": 0,
    "no-undef": 0,
    "no-unused-vars": 0,
    "space-infix-ops": 0,
    "strict": 0
  }
}
